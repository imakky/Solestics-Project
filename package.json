{
  "name": "pages-starter-react-locations",
  "version": "0.0.0",
  "description": "An example repository to help users get started with React development on Yext Pages",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "dev": "source .yextrc && yext init $YEXT_ID -u $UNIVERSE && npm run build:local && yext pages generate-test-data && pages dev",
    "build:local": "vite build",
    "build": "yext pages build && yext pages generate-test-data && yext pages render",
    "build:serve": "npm run build && yext pages serve",
    "serve": "yext pages serve",
    "features": "pages generate features",
    "fmt": "prettier -w .",
    "lint": "eslint .",
    "clean": "rm -rf .artifact-output .yext/ dist/ localData sites-rendered-output",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite"
  },
  "engines": {
    "node": ">=17"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/mapbox-gl": "^2.7.6",
    "@yext/search-core": "2.0.0",
    "@yext/search-headless": "^2.0.0",
    "@yext/search-headless-react": "^2.1.0",
    "@yext/search-ui-react": "^1.1.0-beta.335",
    "fetch-everywhere": "^1.0.5",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-places-autocomplete": "^3.4.0",
    "react-icons": "^4.4.0",
    "react-range": "^1.8.14",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^1.6.0",
    "tailwindcss-animation-delay": "^1.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/lodash": "^4.14.186",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.10",
    "@types/react-slick": "^0.23.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2",
    "@yext/pages": "^1.0.0-beta.6",
    "autoprefixer": "^10.4.8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "generate-license-file": "^1.3.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  }
}
